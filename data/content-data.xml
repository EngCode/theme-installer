		<dc:creator><![CDATA[Abdullah.Ramadan]]></dc:creator>
		<guid isPermaLink="false">https://phenixthemes.com/demo/?post_type=blocks-docs&#038;p=893</guid>
		<description></description>
		<content:encoded><![CDATA[<!-- wp:phenix/group {"metadata":{"categories":["phenix","single"],"patternName":"core/block/1118","name":"Document Section"},"style":{"support":["enable-margin"],"mb":50}} -->
<div class="wp-block-phenix-group mb-50 "><!-- wp:phenix/text {"type":"headline","tagName":"h2","content":"Admin Pages","typography":{"tx-uppercase":"tx-uppercase","size-md":"h1-md"},"style":{"support":["enable-margin"],"mb":20}} -->
<h2 class="wp-block-phenix-text headline tx-uppercase h1-md mb-20">Admin Pages</h2>
<!-- /wp:phenix/text -->

<!-- wp:phenix/text {"content":"to create admin pages with the same design as Phenix Blocks admin pages you can use the pds_add_admin_page function to implement the same design layouts.","style":{"support":["enable-margin"],"mb":"","mt":"","ms":"","me":""}} -->
<p class="wp-block-phenix-text paragraph">to create admin pages with the same design as Phenix Blocks admin pages you can use the pds_add_admin_page function to implement the same design layouts.</p>
<!-- /wp:phenix/text -->

<!-- wp:code {"className":" language-php"} -->
<pre class="wp-block-code  language-php"><code>&lt;?php
//===> Syntax &lt;====//
pds_add_admin_page($page_title, $page_description, $form_page, $tabs_list, $show_submit_button);

//===> Example &lt;====//
$example_page = pds_add_admin_page(
    //==> Page Title &lt;==//
    'Custom Page Example',

    //==> Page Description &lt;==//
    'here you can add and remove Dynamic Custom Data like Post Types.....',

    //==> Options Form Slug &lt;==//
    'pds-page-example',

    //==> Tabs List &lt;==//
    array(
        //==> Locations Panel &lt;==//
        array(
            "title" => "Tabe Example",
            "slug"  => "pds-tab-example",

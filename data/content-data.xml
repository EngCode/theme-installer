<div class="wp-block-phenix-group tab-panel " id="tab-21"><!-- wp:code {"className":"language-js"} -->
<pre class="wp-block-code language-js"><code>//====> Multimedia Lazyloading &lt;====//
Phenix('.px-media').multimedia({
    lazyloading : true, //===> Enable Lazy-Loading
});</code></pre>
<!-- /wp:code --></div>
<!-- /wp:phenix/group --></div>
<!-- /wp:phenix/group --></div>
<!-- /wp:phenix/group --></div>

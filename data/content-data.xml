          \u003c/tr\u003e\n            \u003c!\u002d\u002d ... \u002d\u002d\u003e\n            \u003ctr\u003e\n                \u003ctd class=\u0022has-text-align-left\u0022 data-align=\u0022left\u0022\u003e\u003ccode\u003e.color-primary\u003c/code\u003e\u003c/td\u003e\n                \u003ctd class=\u0022has-text-align-left\u0022 data-align=\u0022left\u0022\u003eset a color of \u003cspan class=\u0022color-primary\u0022\u003eExample\u003c/span\u003e for any text.\u003c/td\u003e\n                \u003ctd class=\u0022has-text-align-center\u0022 data-align=\u0022center\u0022\u003e\u003ccode\u003e\u0026lt;any\u0026gt;\u003c/code\u003e\u003c/td\u003e\n            \u003c/tr\u003e\n            \u003c!\u002d\u002d ... \u002d\u002d\u003e\n            \u003ctr\u003e\n                \u003ctd class=\u0022has-text-align-left\u0022 data-align=\u0022left\u0022\u003e\u003ccode\u003e.color-secondary\u003c/code\u003e\u003c/td\u003e\n                \u003ctd class=\u0022has-text-align-left\u0022 data-align=\u0022left\u0022\u003eset a color of \u003cspan class=\u0022color-secondary\u0022\u003eExample\u003c/span\u003e for any text.\u003c/td\u003e\n                \u003ctd class=\u0022has-text-align-center\u0022 data-align=\u0022center\u0022\u003e\u003ccode\u003e\u0026lt;any\u0026gt;\u003c/code\u003e\u003c/td\u003e\n            \u003c/tr\u003e\n            \u003c!\u002d\u002d ... \u002d\u002d\u003e\n            \u003ctr\u003e\n                \u003ctd class=\u0022has-text-align-left\u0022 data-align=\u0022left\u0022\u003e\u003ccode\u003e.color-success\u003c/code\u003e\u003c/td\u003e\n                \u003ctd class=\u0022has-text-align-left\u0022 data-align=\u0022left\u0022\u003eset a color of \u003cspan class=\u0022color-success\u0022\u003eExample\u003c/span\u003e for any text.\u003c/td\u003e\n                \u003ctd class=\u0022has-text-align-center\u0022 data-align=\u0022center\u0022\u003e\u003ccode\u003e\u0026lt;any\u0026gt;\u003c/code\u003e\u003c/td\u003e\n            \u003c/tr\u0
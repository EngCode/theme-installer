<!-- /wp:phenix/media-element -->

<!-- wp:paragraph -->
<p>Columns Sizes 01 - 12 Max Size No.13 Will take all Available Space</p>
<!-- /wp:paragraph --></div>
<!-- /wp:phenix/column --></div>
<!-- /wp:phenix/row -->


<!-- wp:phenix/text {"content":"in the table below you will find all available class names for flex-box alignment you can use it with our grid system.","style":{"support":["enable-margin"],"mb":"","mt":"","ms":"","me":""}} -->
<p class="wp-block-phenix-text paragraph">in the table below you will find all available class names for flex-box alignment you can use it with our grid system.</p>
<!-- /wp:phenix/text -->

<!-- wp:table -->
<figure class="wp-block-table"><table class="has-fixed-layout"><thead><tr><th>All Directions</th><th>Class Name</th><th>Responsive Class</th><th>Element Type</th></tr></thead><tbody><tr><td>Flex-Start</td><td>.align-start</td><td>align-{breakpoint}-start</td><td>.row,.flexbox</td></tr><tr><td>Flex-Center</td><td>.align-center</td><td>align-{breakpoint}-center</td><td>.row,.flexbox</td></tr><tr><td>Flex-End</td><td>.align-end</td><td>align-{breakpoint}-end</td><td>.row,.flexbox</td></tr><tr><td>Self-Alignment</td><td>.align-self-*</td><td>align-self-{breakpoint}-*</td><td>.col,.col-*</td></tr></tbody></table></figure>

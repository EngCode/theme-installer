		<content:encoded><![CDATA[<!-- wp:phenix/group {"metadata":{"categories":["elements","phenix"],"patternName":"core/block/1536","name":"Document Section"},"style":{"support":["enable-margin"],"mb":50}} -->
<div class="wp-block-phenix-group mb-50 "><!-- wp:phenix/text {"type":"headline","tagName":"h2","content":"Background Coloring ","typography":{"tx-uppercase":"tx-uppercase","size-md":"h1-md"},"style":{"support":["enable-margin"],"mb":20}} -->
<h2 class="wp-block-phenix-text headline tx-uppercase h1-md mb-20">Background Coloring </h2>
<!-- /wp:phenix/text -->

<!-- wp:paragraph -->
<p>Learn how to color your elements background with the background utilities to modify and design your elements easily with shorthand classes</p>
<!-- /wp:paragraph --></div>
<!-- /wp:phenix/group -->

<!-- wp:phenix/group {"metadata":{"categories":["elements","phenix"],"patternName":"core/block/1536","name":"Document Section"},"style":{"support":["enable-margin"],"mb":50}} -->
<div class="wp-block-phenix-group mb-50 "><!-- wp:phenix/text {"type":"headline","tagName":"h2","content":"Colors Customize","typography":{"tx-uppercase":"tx-uppercase","size-md":"h1-md"},"style":{"support":["enable-margin"],"mb":20}} -->
<h2 class="wp-block-phenix-text headline tx-uppercase h1-md mb-20">Colors Customize</h2>
<!-- /wp:phenix/text -->

<!-- wp:paragraph -->
<p>you can customize the colors from the <a href="http://phenix.localhost/test/css-customize/" data-type="post" data-id="516">CSS Customization</a> document with css variables.</p>
<!-- /wp:paragraph --></div>
<!-- /wp:phenix/group -->

<!-- wp:phenix/group {"metadata":{"categories":["elements","phenix"],"patternName":"core/block/1536","name":"Document Section"},"style":{"support":["enable-margin"],"mb":50}} -->
<div class="wp-block-phenix-group mb-50 "><!-- wp:phenix/text {"type":"headline","tagName":"h2","content":"Assets Backgrounds","typography":{"tx-uppercase":"tx-uppercase","size-md":"h1-md"},"style":{"support":["enable-margin"],"mb":20}} -->
<h2 class="wp-block-phenix-text headline tx-uppercase h1-md mb-20">Assets Backgrounds</h2>
<!-- /wp:phenix/text -->

<!-- wp:paragraph -->
<p>in the table below you can find a list of shorthand for the most common <code>background-color</code> property as class names to set a <code>background-color</code> for any element.</p>
<!-- /wp:paragraph -->

<!-- wp:phenix/custom-code {"code":"\u003c!\u002d\u002d WP Table \u002d\u002d\u003e\n\u003cfigure class=\u0022wp-block-table mb-40 docs-table is-style-regular\u0022\u003e\n    \u003ctable\u003e\n        \u003cthead\u003e\n            \u003ctr\u003e\n                \u003cth class=\u0022has-text-align-left\u0022 data-align=\u0022left\u0022\u003eClassname\u003c/th\u003e\n                \u003cth class=\u0022has-text-align-left\u0022 data-align=\u0022left\u0022\u003eDescription\u003c/th\u003e\n                \u003cth class=\u0022has-text-align-center\u0022 data-align=\u0022center\u0022\u003eMarkup\u003c/th\u003e\n            \u003c/tr\u003e\n        \u003c/thead\u003e\n        \u003ctbody\u003e\n            \u003c!\u002d\u002d ... \u002d\u002d\u003e\n            \u003ctr\u003e\n                \u003ctd class=\u0022has-text-align-left\u0022 data-align=\u0022left\u0022\u003e\u003ccode\u003e.bg-parallax\u003c/code\u003e\u003c/td\u003e\n                \u003ctd class=\u0022has-text-align-left\u0022 data-align=\u0022left\u0022\u003eadding parallax effect for any background image.\u003c/td\u003e\n                \u003ctd class=\u0022has-text-align-center\u0022 data-align=\u0022center\u0022\u003e\u003ccode\u003e\u0026lt;any\u0026gt;\u003c/code\u003e\u003c/td\u003e\n            \u003c/tr\u003e\n            \u003c!\u002d\u002d ... \u002d\u002d\u003e\n            \u003ctr\u003e\n                \u003ctd class=\u0022has-text-align-left\u0022 data-align=\u0022left\u0022\u003e\u003ccode\u003e.bg-inherit\u003c/code\u003e\u003c/td\u003e\n                \u003ctd class=\u0022has-text-align-left\u0022 data-align=\u0022left\u0022\u003einherit the background of the parent for any element.\u003c/td\u003e\n                \u003ctd class=\u0022has-text-align-center\u0022 data-align=\u0022center\u0022\u003e\u003ccode\u003e\u0026lt;any\u0026gt;\u003c/code\u003e\u003c/td\u003e\n            \u003c/tr\u003e\n            \u003c!\u002d\u002d ... \u002d\u002d\u003e\n            \u003ctr\u003e\n                \u003ctd class=\u0022has-text-align-left\u0022 data-align=\u0022left\u0022\u003e\u003ccode\u003e.bg-transparent\u003c/code\u003e\u003c/td\u003e\n                \u003ctd class=\u0022has-text-align-left\u0022 data-align=\u0022left\u0022\u003eset a background color of transparent for any element.\u003c/td\u003e\n                \u003ctd class=\u0022has-text-align-center\u0022 data-align=\u0022center\u0022\u003e\u003ccode\u003e\u0026lt;any\u0026gt;\u003c/code\u003e\u003c/td\u003e\n            \u003c/tr\u003e\n            \u003c!\u002d\u002d ... \u002d\u002d\u003e\n            \u003ctr\u003e\n                \u003ctd class=\u0022has-text-align-left\u0022 data-align=\u0022left\u0022\u003e\u003ccode\u003e.bg-primary\u003c/code\u003e\u003c/td\u003e\n                \u003ctd class=\u0022has-text-align-left\u0022 data-align=\u0022left\u0022\u003eset a background color of : \u003cspan class=\u0022pdy-5 pdx-10 me-5 bg-primary\u0022\u003e\u003c/span\u003e for any element.\u003c/td\u003e\n                \u003ctd class=\u0022has-text-align-center\u0022 data-align=\u0022center\u0022\u003e\u003ccode\u003e\u0026lt;any\u0026gt;\u003c/code\u003e\u003c/td\u003e\n            \u003c/tr\u003e\n            \u003c!\u002d\u002d ... \u002d\u002d\u003e\n            \u003ctr\u003e\n                \u003ctd class=\u0022has-text-align-left\u0022 data-align=\u0022left\u0022\u003e\u003ccode\u003e.bg-secondary\u003c/code\u003e\u003c/td\u003e\n                \u003ctd class=\u0022has-text-align-left\u0022 data-align=\u0022left\u0022\u003eset a background color of : \u003cspan class=\u0022pdy-5 pdx-10 me-5 bg-secondary\u0022\u003e\u003c/span\u003e for any element.\u003c/td\u003e\n                \u003ctd class=\u0022has-text-align-center\u0022 data-align=\u0022center\u0022\u003e\u003ccode\u003e\u0026lt;any\u0026gt;\u003c/code\u003e\u003c/td\u003e\n            \u003c/tr\u003e\n            \u003c!\u002d\u002d ... \u002d\u002d\u003e\n            \u003ctr\u003e\n                \u003ctd clas
<figure class="wp-block-table"><table class="has-fixed-layout"><thead><tr><th>Option</th><th>Description</th><th>Default</th><th>Data Type</th></tr></thead><tbody><tr><td><strong>target</strong></td><td>Target to Scroll to</td><td>none</td><td>Selector</td></tr><tr><td><strong>duration</strong></td><td>Scroll Animation Speed by Milliseconds</td><td><code><strong>700</strong></code></td><td>Number</td></tr><tr><td><strong>into</strong></td><td>Increase Target Position by [Number]</td><td>none</td><td>Number</td></tr><tr><td><strong>offset</strong></td><td>Decrease Target Position by [Number]</td><td>none</td><td>Number</td></tr></tbody></table></figure>

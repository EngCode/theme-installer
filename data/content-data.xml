<!-- wp:table -->
<figure class="wp-block-table"><table class="has-fixed-layout"><thead><tr><th>Horizontally</th><th>Class Name</th><th>Responsive Class</th><th>Element Type</th></tr></thead><tbody><tr><td>Flex-Start</td><td>.align-start-x</td><td>align-{breakpoint}-start-x</td><td>.row,.flexbox</td></tr><tr><td>Flex-Center</td><td>.align-center-x</td><td>align-{breakpoint}-center-x</td><td>.row,.flexbox</td></tr><tr><td>Flex-End</td><td>.align-end-x</td><td>align-{breakpoint}-end-x</td><td>.row,.flexbox</td></tr><tr><td>Space-Around</td><td>.align-around</td><td>align-{breakpoint}-around</td><td>.row,.flexbox</td></tr><tr><td>Space-Between</td><td>.align-between</td><td>align-{breakpoint}-between</td><td>.row,.flexbox</td></tr></tbody></table></figure>
<!-- /wp:table -->

<!-- wp:table -->
<figure class="wp-block-table"><table class="has-fixed-layout"><thead><tr><th>Vertically</th><th>Class Name</th><th>Responsive Class</th><th>Element Type</th></tr></thead><tbody><tr><td>Flex-Start</td><td>.align-start-y</td><td>align-{breakpoint}-start-y</td><td>.row,.flexbox</td></tr><tr><td>Flex-Center</td><td>.align-center-y</td><td>align-{breakpoint}-center-y</td><td>.row,.flexbox</td></tr><tr><td>Flex-End</td><td>.align-end-y</td><td>align-{breakpoint}-end-y</td><td>.row,.flexbox</td></tr></tbody></table></figure>
<!-- /wp:table --></div>
<!-- /wp:phenix/group -->]]></content:encoded>

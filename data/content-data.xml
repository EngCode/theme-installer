u003c!\u002d\u002d Menu \u002d\u002d\u003e\n                        \u003cul class=\u0022reset-list\u0022\u003e\n                            \u003cli\u003e\u003ca href=\u0022#\u0022\u003eAnother Link\u003c/a\u003e\u003c/li\u003e\n                            \u003cli\u003e\u003ca href=\u0022#\u0022\u003ePhenix Themes\u003c/a\u003e\u003c/li\u003e\n                            \u003cli\u003e\u003ca href=\u0022#\u0022\u003ePhenix Design\u003c/a\u003e\u003c/li\u003e\n                        \u003c/ul\u003e\n                    \u003c/div\u003e\n                    \u003c!\u002d\u002d Column \u002d\u002d\u003e\n                    \u003cdiv class=\u0022col-12 col-md-4 col-lg-3\u0022\u003e\n                        \u003ca href=\u0022#\u0022\u003e\u003cimg src=\u0022http://via.placeholder.com/300x190\u0022 alt=\u0022\u0022 class=\u0022fluid mb-20\u0022\u003e\u003c/a\u003e\n                        \u003ca href=\u0022#\u0022\u003e\u003cimg src=\u0022http://via.placeholder.com/300x190\u0022 alt=\u0022\u0022 class=\u0022fluid mb-10\u0022\u003e\u003c/a\u003e\n                    \u003c/div\u003e\n                    \u003c!\u002d\u002d // Column \u002d\u002d\u003e\n                \u003c/div\u003e\n                \u003c!\u002d\u002d // Grid \u002d\u002d\u003e\n            \u003c/div\u003e\n            \u003c!\u002d\u002d // Megamenu \u002d\u002d\u003e\n        \u003c/li\u003e\n        \u003cli\u003e\u003ca href=\u0022#\u0022\u003eMenu Item\u003c/a\u003e\u003c/li\u003e\n        \u003cli\u003e\u003ca href=\u0022#\u0022\u003eAnother Link\u003c/a\u003e\u003c/li\u003e\n        \u003cli\u003e\u003ca href=\u0022#\u0022\u003eMultilevel Dropdown\u003c/a\u003e\n            \u003c!\u002d\u002d Submenu [lvl-1] \u002d\u002d\u003e\n            \u003cul\u003e\n                \u003cli\u003e\u003ca href=\u0022#\u0022\u003esubmenu item\u003c/a\u003e\u003c/li\u003e\n                \u003cli\u003e\u003ca href=\u0022#\u0022\u003echeckboxes list\u003c/a\u003e\n                    \u003cul\u003e\n                        \u003cli\u003e\n                            \u003c!\u002d\u002d Option Control [checkbox] \u002d\u002d\u003e\n                            \u003clabel class=\u0022option-control small\u0022 data-type=\u0022checkbox\u0022\u003e\n                                \u003cinput type=\u0022checkbox\u0022 name=\u0022check-1\u0022\u003e\n                                \u003cspan class=\u0022fas fa-check\u0022\u003eCheckbox Example\u003c/span\u003e\n                            \u003c/label\u003e\n                            \u003c!\u002d\u002d Option Control [checkbox] \u002d\u002d\u003e\n                        \u003c/li\u003e\n                        \u003cli\u003e\n                            \u003c!\u002d\u002d Option Control [checkbox] \u002d\u002d\u003e\n                            \u003clabel class=\u0022option-control small\u0022 data-type=\u0022checkbox\u0022\u003e\n                                \u003cinput type=\u0022checkbox\u0022 name=\u0022check-1\u0022\u003e\n                                \u003cspan class=\u0022fas fa-check\u0022\u003eCheckbox Example\u003c/span\u003e\n                            \u003c/label\u003e\n                            \u003c!\u002d\u002d Option Control [checkbox] \u002d\u002d\u003e\n                        \u003c/li\u003e\n                        \u003cli\u003e\n                            \u003c!\u002d\u002d Option Control [checkbox] \u002d\u002d\u003e\n                            \u003clabel class=\u0022option-control small\u0022 data-type=\u0022checkbox\u0022\u003e\n                                \u003cinput type=\u0022checkbox\u0022 name=\u0022check-1\u0022\u003e\n                                \u003cspan class=\u0022fas fa-check\u0022\u003eCheckbox Example\u003c/span\u003e\n                            \u003c/label\u003e\n                            \u003c!\u002d\u002d Option Control [checkbox] \u002d\u002d\u003e\n                        \u003c/li\u003e\n                    \u003c/ul\u003e\n                
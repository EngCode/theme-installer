		<content:encoded><![CDATA[<!-- wp:phenix/group {"metadata":{"categories":["phenix","single"],"patternName":"core/block/1118","name":"Document Section"},"style":{"support":["enable-margin"],"mb":50}} -->
<div class="wp-block-phenix-group mb-50 "><!-- wp:phenix/text {"type":"headline","tagName":"h2","content":"Cards","typography":{"tx-uppercase":"tx-uppercase","size-md":"h1-md"},"style":{"support":["enable-margin"],"mb":20}} -->

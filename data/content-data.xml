		<description></description>
		<content:encoded><![CDATA[<!-- wp:phenix/group {"metadata":{"categories":["phenix","single"],"patternName":"core/block/1118","name":"Document Section"},"style":{"support":["enable-margin"],"mb":50}} -->
<div class="wp-block-phenix-group mb-50 "><!-- wp:phenix/text {"type":"headline","tagName":"h2","content":"Buttons","typography":{"tx-uppercase":"tx-uppercase","size-md":"h1-md"},"style":{"support":["enable-margin"],"mb":20}} -->
<h2 class="wp-block-phenix-text headline tx-uppercase h1-md mb-20">Buttons</h2>
<!-- /wp:phenix/text -->

<!-- wp:phenix/text {"content":"Documentation and examples for styles of button component of Phenix Design System.","style":{"support":["enable-margin"],"mb":"","mt":"","ms":"","me":""}} -->
<p class="wp-block-phenix-text paragraph">Documentation and examples for styles of button component of Phenix Design System.</p>
<!-- /wp:phenix/text --></div>
<!-- /wp:phenix/group -->

<!-- wp:phenix/group {"metadata":{"categories":["phenix","single"],"patternName":"core/block/1118","name":"Document Section"},"style":{"support":["enable-margin"],"mb":50}} -->
<div class="wp-block-phenix-group mb-50 "><!-- wp:phenix/text {"type":"headline","tagName":"h2","content":"Overview","typography":{"tx-uppercase":"tx-uppercase","size-md":"h1-md"},"style":{"support":["enable-margin"],"mb":20}} -->
<h2 class="wp-block-phenix-text headline tx-uppercase h1-md mb-20">Overview</h2>
<!-- /wp:phenix/text -->

<!-- wp:phenix/text {"content":"PDS buttons are \u003cstrong\u003epreset\u003c/strong\u003e style, Add the base class \u003ccode\u003e.btn\u003c/code\u003e to any \u003ccode\u003e\u0026lt;element\u003e\u003c/code\u003e, then extend with our optional modifier classes or custom styles or via CSS variables that comes with it,","style":{"support":["enable-margin"],"mb":"","mt":"","ms":"","me":""}} -->
<p class="wp-block-phenix-text paragraph">PDS buttons are <strong>preset</strong> style, Add the base class <code>.btn</code> to any <code>&lt;element></code>, then extend with our optional modifier classes or custom styles or via CSS variables that comes with it,</p>
<!-- /wp:phenix/text --></div>
<!-- /wp:phenix/group -->

<!-- wp:phenix/group {"metadata":{"categories":["elements","phenix"],"patternName":"core/block/1536","name":"Document Section"},"style":{"support":["enable-margin"],"mb":50}} -->
<div class="wp-block-phenix-group mb-50 "><!-- wp:phenix/text {"type":"headline","tagName":"h2","content":"Standard Buttons","typography":{"tx-uppercase":"tx-uppercase","size-md":"h1-md"},"style":{"support":["enable-margin"],"mb":20}} -->
<h2 class="wp-block-phenix-text headline tx-uppercase h1-md mb-20">Standard Buttons</h2>
<!-- /wp:phenix/text -->

<!-- wp:paragraph -->
<p>in the example below you can find the normal size button with multiple colors and the CSS variables to customize it as you desire.</p>
<!-- /wp:paragraph -->

<!-- wp:phenix/custom-code {"code":"\u003c!\u002d\u002d Button \u002d\u002d\u003e\n\u003cbutton class=\u0022btn me-10 mb-15 border-alpha-10\u0022\u003eButton\u003c/button\u003e\n\n\u003c!\u002d\u002d Button \u002d\u002d\u003e\n\u003cbutton class=\u0022btn primary me-10 mb-15\u0022\u003ePrimary\u003c/button\u003e\n\n\u003c!\u002d\u002d Button \u002d\u002d\u003e\n\u003cbutton class=\u0022btn secondary me-10 mb-15\u0022\u003eSecondary\u003c/button\u003e\n\n\u003c!\u002d\u002d Button \u002d\u002d\u003e\n\u003cbutton class=\u0022btn info me-10 mb-15\u0022\u003eInformative\u003c/button\u003e\n\n\u003c!\u002d\u002d Button \u002d\u002d\u003e\n\u003cbutton class=\u0022btn disabled me-10 mb-15\u0022 \u0022=\u0022\u0022\u003eDisabled\u003c/button\u003e\n\n\u003cbr\u003e\n\n\u003c!\u002d\u002d Button \u002d\u002d\u003e\n\u003cbutton class=\u0022btn success me-10 mb-15\u0022\u003eSuccess\u003c/button\u003e\n\n\u003c!\u002d\u002d Button \u002d\u002d\u003e\n\u003cbutton class=\u0022btn dark me-10 mb-15\u0022\u003eDark\u003c/button\u003e\n\n\u003c!\u002d\u002d Button \u002d\u002d\u003e\n\u003cbutton class=\u0022btn danger me-10 mb-15\u0022\u003eDanger\u003c/button\u003e\n\n\u003c!\u002d\u002d Button \u002d\u002d\u003e\n\u003cbutton class=\u0022btn warning me-10 mb-15\u0022\u003eWarning\u003c/button\u003e\n\n\u003c!\u002d\u002d Button \u002d\u002d\u003e\n\u003cbutton class=\u0022btn light me-10 mb-15\u0022\u003eLight\u003c/button\u003e"} /-->

<!-- wp:phenix/group {"metadata":{"name":"Tabs","categories":["elements","phenix","single"],"patternName":"core/block/1385"},"className":"px-tabs"} -->
<div class="wp-block-phenix-group px-tabs "><!-- wp:phenix/group {"isFlexbox":true,"className":"tabs-navigation documentation"} -->
<div class="wp-block-phenix-group tabs-navigation documentation  flexbox"><!-- wp:phenix/button {"isLink":true,"label":"HTML","url":"#tab-1","typography":{"tx-uppercase":"tx-uppercase"},"style":{"background":{"type":"color","rotate":null,"value":"bg-gray"},"support":["enable-margin"]},"responsive":{"size":"small"}} -->
<a class="wp-block-phenix-button btn tx-uppercase gray small  normal" rel="noopener" href="#tab-1">HTML</a>
<!-- /wp:phenix/button -->

<!-- wp:phenix/button {"isLink":true,"label":"CSS","url":"#tab-2","typography":{"tx-uppercase":"tx-uppercase"},"style":{"background":{"type":"color","rotate":null,"value":"bg-gray"},"support":["enable-margin"]},"responsive":{"size":"small"}} -->
<a class="wp-block-phenix-button btn tx-uppercase gray small  normal" rel="noopener" href="#tab-2">CSS</a>
<!-- /wp:phenix/button --></div>
<!-- /wp:phenix/group -->

<!-- wp:phenix/group {"className":"tabs-panels"} -->
<div class="wp-block-phenix-group tabs-panels "><!-- wp:phenix/group {"id":"tab-1","className":"tab-panel"} -->
<div class="wp-block-phenix-group tab-panel " id="tab-1"><!-- wp:code {"className":"language-html"} -->
<pre class="wp-block-code language-html"><code>&lt;!-- Button -->
&lt;button class="btn border-alpha-10">Button&lt;/button>

&lt;!-- Button -->
&lt;button class="btn primary">Primary&lt;/button>

&lt;!-- Button -->
&lt;button class="btn secondary">Secondary&lt;/button>

&lt;!-- Button -->
&lt;button class="btn info">Informative&lt;/button>

&lt;!-- Button -->
&lt;button class="btn success">Success&lt;/button>

&lt;!-- Button -->
&lt;button class="btn dark">Dark&lt;/button>

&lt;!-- Button -->
&lt;button class="btn danger">Danger&lt;/button>

&lt;!-- Button -->
&lt;button class="btn warning">Warning&lt;/button>

&lt;!-- Button -->
&lt;button class="btn light">Light&lt;/button>

&lt;!-- Button -->
&lt;button class="btn disabled">Light&lt;/button></code></pre>
<!-- /wp:code --></div>
<!-- /wp:phenix/group -->

<!-- wp:phenix/group {"id":"tab-2","className":"tab-panel"} -->
<div class="wp-block-phenix-group tab-panel " id="tab-2"><!-- wp:code {"className":"language-css"} -->
<pre class="wp-block-code language-css"><code>/*==== CSS Options ====*/
.btn {
    /*==== Options ====*/

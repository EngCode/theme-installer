<!-- /wp:phenix/media-element -->

<!-- wp:paragraph -->
<p>Flexbox layout options</p>
<!-- /wp:paragraph --></div>
<!-- /wp:phenix/column -->

<!-- wp:phenix/column {"size":"12","typography":{"align":"tx-align-center"},"responsive":{"size-md":"6","size-lg":"4"}} -->
<div class="wp-block-phenix-column col-12 tx-align-center col-md-6 col-lg-4 "><!-- wp:phenix/media-element {"setting":{"src":"https://phenixthemes.com/demo/wp-content/uploads/2025/01/grid-ops-png.avif"},"style":{"support":["enable-margin"],"mb":"","mt":"","ms":"","me":""},"url":"https://phenixthemes.com/demo/wp-content/uploads/2025/01/05-toolbar-options-highlight.avif","isLightBox":true,"lightbox_src":true} -->
